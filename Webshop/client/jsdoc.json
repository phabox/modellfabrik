{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./src"],
        "includePattern": "\\.(vue|js)$"
    },
    "plugins": [
        "node_modules/jsdoc-vuejs",
        "plugins/markdown",
        "node_modules/better-docs/category"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "template": "node_modules/better-docs"
    },
    "templates":{
        "cleverLinks": true,
        "search": true
    }
  }